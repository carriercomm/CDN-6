#!/bin/bash
port=  origin=  name= username= keyfile=

while getopts p:o:n:u:i opt; do
  case $opt in
  p)
      port=$OPTARG
      ;;
  o)
      origin=$OPTARG
      ;;
  n)
      name=$OPTARG
      ;;
  u)
      username=$OPTARG
      ;;
  i)
      keyfile=$OPTARG
      ;;
  esac
done

shift $((OPTIND - 1))

ssh -i ~/.ssh/cs5700_rsa abarba@ec2-54-174-6-90.compute-1.amazonaws.com "bash -s" -- < ./scripts/deploy